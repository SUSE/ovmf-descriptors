{
    "description": "UEFI firmware for x86_64, with Secure Boot, SMM, and openSUSE certs enrolled",
    "interface-types": [
        "uefi"
    ],
    "mapping": {
        "device": "flash",
        "executable": {
            "filename": "@DATADIR@/ovmf-x86_64-smm-opensuse-code.bin",
            "format": "raw"
        },
        "nvram-template": {
            "filename": "@DATADIR@/ovmf-x86_64-smm-opensuse-vars.bin",
            "format": "raw"
        }
    },
    "targets": [
        {
            "architecture": "x86_64",
            "machines": [
                "pc-q35-*"
            ]
        }
    ],
    "features": [
        "acpi-s3",
        "acpi-s4",
        "amd-sev",
        "requires-smm",
        "secure-boot",
	"enrolled-keys",
        "verbose-dynamic"
    ],
    "tags": [

    ]
}
