{
    "description": "UEFI firmware for riscv (riscv64)",
    "interface-types": [
        "uefi"
    ],
    "mapping": {
        "device": "flash",
        "executable": {
            "filename": "@DATADIR@/ovmf-riscv64-code.bin",
            "format": "raw"
        },
        "nvram-template": {
            "filename": "@DATADIR@/ovmf-riscv64-vars.bin",
            "format": "raw"
        }
    },
    "targets": [
        {
            "architecture": "riscv64",
            "machines": [
                "virt-*"
            ]
        }
    ],
    "features": [
        "verbose-static"
    ],
    "tags": [

    ]
}
