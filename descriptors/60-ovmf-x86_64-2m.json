{
    "description": "UEFI firmware for x86_64 (legacy 2MB flash size)",
    "interface-types": [
        "uefi"
    ],
    "mapping": {
        "device": "flash",
        "executable": {
            "filename": "@DATADIR@/ovmf-x86_64-code.bin",
            "format": "raw"
        },
        "nvram-template": {
            "filename": "@DATADIR@/ovmf-x86_64-vars.bin",
            "format": "raw"
        }
    },
    "targets": [
        {
            "architecture": "x86_64",
            "machines": [
                "pc-i440fx-*",
                "pc-q35-*"
            ]
        }
    ],
    "features": [
        "acpi-s3",
        "acpi-s4",
        "amd-sev",
        "amd-sev-es",
        "verbose-dynamic"
    ],
    "tags": [

    ]
}
